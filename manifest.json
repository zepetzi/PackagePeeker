{
	"manifest_version": 3,
	"name": "PackagePeeker",
	"version": "0.1",
	"permissions": ["storage", "activeTab"],
	"content_security_policy": {
        "script-src": "'self'",
        "object-src": "'self'"
    },
	"action": {
		"default_popup": "popup.html"
	},

	"icons" : {

	},
	
	"content_scripts": [
		{
		  "matches": ["<all_urls>"],
		  "js": ["popup.js"]
		}
	],
	
	"background": {
		"service_worker": "background.js"
	}
}

